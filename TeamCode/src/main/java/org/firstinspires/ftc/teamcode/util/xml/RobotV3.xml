<?xml version='1.0' encoding='UTF-8' standalone='yes' ?>
<Robot type="FirstInspires-FTC">
    <DeviceInterfaceModule name="dim" serialNumber="AL00XKYB">
        <I2cDevice name="navx" port="0" />
        <ModernRoboticsI2cRangeSensor name="rangeSensor1" port="2" />
        <ColorSensor name="colorSensor1" port="4" />
        <OpticalDistanceSensor name="ods" port="1" />
    </DeviceInterfaceModule>
    <MotorController name="Right Motor Controller" serialNumber="AL00VSAM">
        <Motor name="fMotorR" port="1" />
        <Motor name="rMotorR" port="2" />
    </MotorController>
    <ServoController name="Servo Controller 2" serialNumber="AL00XLTB">
        <ContinuousRotationServo name="liftServo" port="1" />
        <ContinuousRotationServo name="buttonServo" port="3" />
        <Servo name="particleReleaseServo" port="5" />
    </ServoController>
    <MotorController name="lift n shooter Motor Controller" serialNumber="AL00VBJK">
        <Motor name="liftMotor" port="1" />
        <Motor name="partAccMotor" port="2" />
    </MotorController>
    <MotorController name="Harvester Controller" serialNumber="AL00VS98">
        <Motor name="harvesterMotor" port="1" />
    </MotorController>
    <MotorController name="Left Motor Controller" serialNumber="AL00YCFW">
        <Motor name="fMotorL" port="1" />
        <Motor name="rMotorL" port="2" />
    </MotorController>
</Robot>
